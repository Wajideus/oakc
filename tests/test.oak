#include "stdio", "stdlib"


const Actor actors[16]


typedef struct ActorInstance of
    mixin fixto actors via int actor
end


typedef enum Status of
    DEAD = 1 << 2
    BURNED, FROZEN
end

typedef struct Entity of
    Component *component[]
    Status status
end

typedef struct Door of
    mixin Entity entity
    float openness
end

